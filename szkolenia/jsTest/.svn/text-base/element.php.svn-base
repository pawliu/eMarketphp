<?php
function dajElementy() {
	return array (
			new Element ( "1", "Element 1", "Kod 1" ),
			new Element ( "2", "Element 2", "Kod 2" )
	);
}
class Element {
	public $id;
	public $nazwa;
	public $kod;
	public function __construct($id, $nazwa, $kod) {
		$this->id = $id;
		$this->nazwa = $nazwa;
		$this->kod = $kod;
	}
}
